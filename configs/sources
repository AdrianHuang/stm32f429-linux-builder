# source defintions

uboot_version := u-boot_ARMv7M
kernel_version := linux_ARMv7M
busybox_version := busybox-1.22.1

FETCH_CMD_uboot := git clone https://github.com/AdrianHuang/u-boot_ARMv7M.git
FETCH_CMD_kernel := git clone https://github.com/AdrianHuang/linux_ARMv7M.git
FETCH_CMD_busybox := wget -P downloads -c http://busybox.net/downloads/${busybox_version}.tar.bz2
